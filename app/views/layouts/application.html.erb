<!DOCTYPE html>
<html>
<head>
  <title>Quenchd</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%=render 'layouts/header' unless @disable_header %>

<% if flash %>
  <% flash.each do |key, values| %>
    <% values.each do |message|%>
      <h5><%= message %></h5>
    <% end %>
  <% end %>
<% end %>



<%= yield %>


</body>
</html>
